<?xml version="1.0" encoding="UTF-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="#ff4e89e3" android:endColor="#ff4e89e3" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff4e89e3" android:endColor="#ff4e89e3" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ff3372d4" android:endColor="#ff3372d4" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
</selector>